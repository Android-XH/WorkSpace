<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomMyProgress">
        <attr name="cus_progress_current" format="integer"/>
        <attr name="cus_progress_max" format="integer"/>

        <attr name="cus_progress_unreached_color" format="color"/>
        <attr name="cus_progress_reached_start_color" format="color"/>
        <attr name="cus_progress_reached_color" format="color"/>
        <attr name="cus_progress_reached_end_color" format="color"/>
        <attr name="cus_progress_reached_bar_height" format="dimension"/>
        <attr name="cus_progress_unreached_bar_height" format="dimension"/>
        <attr name="cus_inner_drawable" format="reference"/>
        <attr name="cus_run_rate" format="enum">
            <enum name="fast" value="200"/>
            <enum name="medium" value="300"/>
            <enum name="slow" value="500"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomCircleProgress">
        <attr name="circle_custom_progress" format="float"/>
        <attr name="circle_progress_max" format="integer"/>
        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>
        <attr name="circle_finished_default_color" format="color"/>
        <attr name="circle_background_color" format="color"/>
        <attr name="circle_finished_stroke_width" format="dimension"/>
        <attr name="circle_unfinished_stroke_width" format="dimension"/>
        <attr name="circle_starting_degree" format="integer" />
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="text_title_size" format="dimension"/>
        <attr name="text_title_color" format="color"/>
        <attr name="progress_text_size" format="dimension"/>
        <attr name="progress_text_color" format="color"/>
        <attr name="background_color" format="color"/>
        <attr name="show_text" format="boolean"/>
        <attr name="text" format="string"/>
        <attr name="title" format="string"/>
        <attr name="type" format="enum">
            <enum name="progress" value="-1"/>
            <enum name="text" value="0"/>
            <enum name="tag" value="1"/>
            <enum name="tag_text" value="2"/>
        </attr>

    </declare-styleable>
</resources>